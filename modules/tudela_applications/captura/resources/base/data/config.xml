<config base = "swaf_professional_application.config">
<framework_data>
    <!--  specify which ACE to use to view each spatial_context type  -->
    <spatial_context  
        name="gis"
        ace_name="Default"
        universe_name="gis" />
    <spatial_context
        name="internals"
        ace_name="Substation Internals"
        universe_name="internals"/>
    <spatial_context
        name="drawing"
        ace_name="drawing"
        universe_name="drawing"/>
  </framework_data>

  <plugins>
        <super/>

	<plugin name="maps" class_name="map_manager">
	     <!-- requerido por google_maps -->
	    <ace_gui ace_name="Default" role_names="add_google_maps_role"/>
	    <!-- fin requerido por google_maps -->

	    <property name="render_priority" value="-30"/>
	    <property name="initial_interaction_mode" value="select" />
	    <interaction_modes>
		<mode name="select" />
		<mode name="geometry" />
		<mode name="zoom_in" />
		<mode name="zoom_out" />
		<mode name="pan" />
		<cycle_sequence modes="select,geometry"/>
	    </interaction_modes>
	    <keyboard_interaction>
		<key id="delete_key" modifier="0" mode="select" action="delete_selection_with_confirmation"/>
		<key id="delete_key" modifier="0" mode="select_pan" action="delete_selection_with_confirmation"/>
		<key id="delete_key" modifier="0" mode="zoom_in" action="delete_selection_with_confirmation"/>
		<key id="delete_key" modifier="0" mode="zoom_out" action="delete_selection_with_confirmation"/>
		<key id="delete_key" modifier="0" mode="pan" action="delete_selection_with_confirmation"/>
	    </keyboard_interaction>
	</plugin>

	<plugin name="editor_manager" class_name="editor_manager" >
	    <properties>
		<property name="direction_render_priority" value="40"/>
		<property name="highlight_render_priority" value="85"/>
		<property name="editor_gui_definition_module_name" value="swaf_professional_application"/>
		<property name="editor_gui_definition_file_name" value="editor_gui.xml"/>
		<property name="configuration_file_name" value="captura.collection_config.xml"/>
	    </properties>
	</plugin>

	<plugin name="explorer" class_name="explorer_plugin">
            <properties>
	        <property name="floating_gui_definition_module_name" value="swaf_professional_application"/>
	        <property name="floating_gui_definition_file_name"   value="floating_explorer_view_gui.xml"/>
	        <property name="embedded_gui_definition_module_name" value="swaf_professional_application"/>
	        <property name="embedded_gui_definition_file_name"   value="embedded_explorer_view_gui.xml"/>
	        <property name="expose_embedded_selection"           value="true"/>
	        <property name="expose_floating_selection"           value="true"/>
		<property name="model_config_definition_file_name"   value="explorer_model_config.xml"/>
		<property name="model_config_definition_module_name" value="captura"/>
            <property name="editor_manager_name"                 value="editor_manager"/>
            </properties>
            <export>
	        <toolbar source_name="find"/>
	        <toolbar source_name="advanced_find"/>
            </export>
            <find_engine>
	        <categories>
				<category name="fondo_texto" default="yes"/>
				<category name="callejero" />
				<category name="drafting_text" />
	        </categories>
                <engine plugin_name="search_engine"/>
                <default_text id ="find_text"/>
            </find_engine>
        </plugin>
   
    <plugin name="panes" class_name="pane_manager">
      <properties>
		<!-- list of role names which, when assigned to elements of the gui, 
		     will put them under the control of the pane_manager -->
		<property name="role_names" value="validacion,editor_manager, object_control, themes, modelos_carga, viewers"/>
      </properties> 
    </plugin>
   
    <!-- paneles de opciones globales -->

    <plugin name = "rwo_specials" class_name = "rwo_specials_plugin"/>

 
    <!-- AÃ±adidos plugins de DXF translator -->
    <plugin name="dxf" class_name="dxf_mapping_plugin">
	<properties>
	    <property name="dxf_soc_def_name" value="dxf_soc_def" />
	</properties>
    </plugin>    
	
    <plugin name="soms" class_name="sas_soms_swaf_plugin">
	    <properties>
		<property name="map_manager_name" value="maps"/>
	    </properties>
    </plugin>
	
    <plugin name="traductor" class_name="traductor_plugin"/>
    <plugin name="cargador_topo" class_name="cargador_topo_plugin"/>

